<h1>All specialties</h1>
<%= render 'shared/navbar' %>
<% @specialties.each do |specialty| %>
<div class="container">
  <div class="row m-0">
    <div class="col-5 p-0">
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://static.cuisineaz.com/240x192/i119038-.jpeg)">
        <div class="specialty-name"><%=specialty.name %></div>
        <a href="#"><i class="far fa-heart favorites"></i></a>
      </div>
    </div>
    <div class="col-5 p-0">
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://static.cuisineaz.com/240x192/i119038-.jpeg)">
        <div class="specialty-name"><%=specialty.name %></div>
        <a href="#"><i class="far fa-heart favorites"></i></a>
      </div>
    </div>
  </div>
</div>
<% end %>



